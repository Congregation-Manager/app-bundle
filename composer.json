{
    "name": "congregation-manager/app-bundle",
    "type": "library",
    "description": "Congregation Manager application on Symfony.",
    "keywords": [
        "app",
        "congregation-manager"
    ],
    "homepage": "https://github.com/congregation-manager/congregation-manager",
    "license": "proprietary",
    "authors": [
        {
            "name": "Lorenzo Ruozzi",
            "homepage": "https://lruozzi9.github.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "CongregationManager\\Bundle\\App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CongregationManager\\Bundle\\App\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
